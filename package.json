{
  "name": "@artsy/detect-responsive-traits",
  "version": "0.0.4",
  "description": "Get responsive size traits from a user agent",
  "main": "build/index.js",
  "author": "Art.sy Inc <it@artsymail.com>",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@artsy/auto-config": "0.1.0",
    "@types/jest": "23.3.9",
    "@types/node": "10.12.9",
    "danger": "6.1.4",
    "husky": "1.1.4",
    "jest": "23.6.0",
    "lint-staged": "8.0.5",
    "prettier": "1.17.0",
    "ts-jest": "23.10.4",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.4.5"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "test": "jest",
    "prepack": "yarn build",
    "filter-data:not-android": "ts-node scripts/filter-data-set.ts data/not-windows-macos-x11-or-android.json",
    "filter-data:not-ios": "ts-node scripts/filter-data-set.ts data/not-windows-macos-x11-or-ios.json",
    "filter-data": "yarn filter-data:not-android && yarn filter-data:not-ios"
  },
  "repository": "https://github.com/artsy/detect-responsive-traits.git",
  "jest": {
    "preset": "ts-jest"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "proseWrap": "always"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn build"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "tslint --fix",
      "yarn prettier --write",
      "git add"
    ]
  }
}

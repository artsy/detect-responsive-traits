{
  "name": "@artsy/detect-responsive-traits",
  "version": "0.0.5",
  "description": "Get responsive size traits from a user agent",
  "main": "build/index.js",
  "author": "Art.sy Inc <it@artsymail.com>",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@artsy/auto-config": "1.0.1",
    "@types/jest": "24.0.12",
    "@types/node": "10.14.6",
    "danger": "7.1.3",
    "husky": "3.1.0",
    "jest": "24.7.1",
    "lint-staged": "8.1.5",
    "prettier": "1.17.0",
    "ts-jest": "23.10.5",
    "ts-node": "7.0.1",
    "tslint": "5.16.0",
    "typescript": "3.7.4"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "test": "jest",
    "prepack": "yarn build",
    "filter-data:not-android": "ts-node scripts/filter-data-set.ts data/not-windows-macos-x11-or-android.json",
    "filter-data:not-ios": "ts-node scripts/filter-data-set.ts data/not-windows-macos-x11-or-ios.json",
    "filter-data": "yarn filter-data:not-android && yarn filter-data:not-ios"
  },
  "repository": "https://github.com/artsy/detect-responsive-traits.git",
  "jest": {
    "preset": "ts-jest"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "proseWrap": "always"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn build"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "tslint --fix",
      "yarn prettier --write",
      "git add"
    ]
  }
}
